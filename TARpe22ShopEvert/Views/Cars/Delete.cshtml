@model TARpe22ShopEvert.Models.Cars.CarDeleteViewModel
<h1>Delete</h1>
<div>
    <h4>Car</h4>
    <hr/>
    <dl class="row">
        <div class="form-group-row">
            <label asp-for="Id" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Id)
        </div>
        
        <div class="form-group-row">
            <label asp-for="Mark" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Mark)
        </div>
        
        <div class="form-group-row">
            <label asp-for="Price" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Price)
        </div>
        
        <div class="form-group-row">
            <label asp-for="Model" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Model)
        </div>
        
        <div class="form-group-row">
            <label asp-for="IsNew" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.IsNew)
        </div>
        
        <div class="form-group-row">
            <label asp-for="HorsePower" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.HorsePower)
        </div>
        
        <div class="form-group-row">
            <label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.CreatedAt)
        </div>

        @foreach (var images in Model.FileToApiViewModelCar)
        {
            <partial name="_Images" model="images" />
        }
    </dl>
</div>
<hr/>
<form asp-action="DeleteConfirmation">
    <input type="hidden" asp-for="Id"/>
    <input type="submit" value="Delete" class="btn btn-danger"/>
    <a asp-action="index" asp-controller="realEstates" class="btn btn-primary">Back</a>
</form>